{
  "name": "awalhadi/laravel-toastr",
  "description": "Easy Toastr notifications for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Awal Hadi",
      "email": "your.email@example.com"
    }
  ],
  "require": {
    "php": "^8.1|^8.2",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
    "phpunit/phpunit": "^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Awalhadi\\LaravelToastr\\": "src/"
    },
    "files": [
      "src/toastr_helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Awalhadi\\LaravelToastr\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Awalhadi\\LaravelToastr\\ToastrServiceProvider"
      ],
      "aliases": {
        "Toastr": "Awalhadi\\LaravelToastr\\Facades\\Toastr"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
